{
	"name": "dcupush",
	"displayName": "DCU-push",
	"description": "DCU push command for oracle commerce cloud",
	"author": {
		"name": "dhirajsriram"
	  },
	  "icon": "images/icon.png",
	"version": "0.0.3",
	"publisher": "Dhirajsriram",
	"engines": {
		"vscode": "^1.30.0"
	},
	"enableProposedApi": false,
	"repository": {
		"type": "git",
		"url": "https://github.com/dhirajsriram/DCU"
	  },
	  "bugs": {
		"url": "https://github.com/dhirajsriram/DCU/issues"
	  },
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:terminalTest.dcu"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "terminalTest.dcu",
				"title": "DCU push"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "tslint ./src/*.ts",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/node": "^6.0.40",
		"tslint": "^5.11.0",
		"typescript": "^2.0.3",
		"vscode": "^1.1.22"
	}
}
